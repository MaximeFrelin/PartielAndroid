<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="bm.iut.informatique.partielandroid.vues.FragmentHome">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <TextView
            android:id="@+id/id_dummy_location"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/id_circle_dawn"
            android:layout_marginStart="25dp"
            android:layout_marginTop="59dp"
            android:layout_toEndOf="@+id/textView5" />

        <TextView
            android:id="@+id/textView6"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/titre_is_circle_dawn" />

        <TextView
            android:id="@+id/id_circle_dawn"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_alignParentTop="true"
            android:layout_toEndOf="@+id/textView5"
            android:layout_weight="1" />

        <TextView
            android:id="@+id/textView5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBaseline="@+id/id_dummy_location"
            android:layout_alignBottom="@+id/id_dummy_location"
            android:layout_alignParentStart="true"
            android:layout_weight="1"
            android:text="@string/titre_dummy_location" />
    </RelativeLayout>

</FrameLayout>
